<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#20B2AA"
    >
<TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_gravity="center_horizontal"
    android:text="@string/sensorThemeName"
    android:textSize="50sp"
    android:textStyle="bold"
    android:paddingStart="100dp"
    android:paddingEnd="50dp"
    />
        <TableLayout
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:layout_gravity="center_horizontal"
            android:background="#969696"
            android:paddingBottom="2dp"
            android:paddingStart="2dp"
            android:paddingEnd="1dp"
            android:paddingTop="5dp"
            android:layout_weight="1"
            android:shrinkColumns="0,1,2"
            >
            <TableRow
                >
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="230dp"
                    android:orientation="vertical"
                    android:background="@drawable/side">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:text="@string/shell"
                        android:textSize="20sp"
                        android:textStyle="bold"/>
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        >
                        <TextView
                            android:id="@+id/leaveWorkpiecePosSS"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/leaveWorkPiecePosStr"
                            android:gravity="center"
                            android:drawableTop="@drawable/tvselector"
                            />
                        <TextView
                            android:id="@+id/grabMaterialsPosSS"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/grabMaterialsPosStr"
                            android:gravity="center"
                            android:drawableTop="@drawable/tvselector"
                            />
                        <TextView
                            android:id="@+id/shellSS"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/shellStr"
                            android:gravity="center"
                            android:drawableTop="@drawable/tvselector"
                            />
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="230dp"
                    android:orientation="vertical"
                    android:background="@drawable/side">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:text="@string/batteryCover"
                        android:textSize="20sp"
                        android:textStyle="bold"/>
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        >
                        <TextView
                            android:id="@+id/leaveBatteryCoverPosSS"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/leaveWorkPiecePosStr"
                            android:gravity="center"
                            android:drawableTop="@drawable/tvselector"
                            />
                        <TextView
                            android:id="@+id/grabBatteryCoverPosSS"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/grabMaterialsPosStr"
                            android:gravity="center"
                            android:drawableTop="@drawable/tvselector"
                            />
                        <TextView
                            android:id="@+id/BatteryCoverSS"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/workPiece"
                            android:gravity="center"
                            android:drawableTop="@drawable/tvselector"
                            />
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="230dp"
                    android:orientation="vertical"
                    android:background="@drawable/side">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:text="@string/armStateStr"
                        android:textSize="20sp"
                        android:textStyle="bold"/>
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        >
                        <TextView
                            android:id="@+id/armReady"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/armReadyStr"
                            android:gravity="center"
                            android:drawableTop="@drawable/tvselector"
                            />
                        <TextView
                            android:id="@+id/armRunning"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/armRunningStr"
                            android:gravity="center"
                            android:drawableTop="@drawable/tvselector"
                            />
                        <TextView
                            android:id="@+id/grablockBatteryCoverPosSS"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/otherStr"
                            android:gravity="center"
                            android:drawableTop="@drawable/tvselector"
                            />
                        <TextView
                            android:id="@+id/lockBatteryCover"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/otherStr"
                            android:gravity="center"
                            android:drawableTop="@drawable/tvselector"
                            />

                    </LinearLayout>
                </LinearLayout>
            </TableRow>
            <TableRow>
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="460dp"
                    android:orientation="vertical"
                    android:background="@drawable/side">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:text="@string/loadBatteryStr"
                        android:textSize="20sp"
                        android:textStyle="bold"/>
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        >
                        <TextView
                            android:id="@+id/leaveUltrasonicPosSS"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/workPiece"
                            android:gravity="center"
                            android:drawableTop="@drawable/tvselector"
                            />
                        <TextView
                            android:id="@+id/grabUltrasonicPosSS"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/batteryStr"
                            android:gravity="center"
                            android:drawableTop="@drawable/tvselector"
                            />
                        <TextView
                            android:id="@+id/ultrasonicSS"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/lackBatteryStr"
                            android:gravity="center"
                            android:drawableTop="@drawable/tvselector"
                            />

                    </LinearLayout>
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    >
                    <TextView
                        android:id="@+id/ultrasonicHomeSS"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/pushBatteryPosStr"
                        android:gravity="center"
                        android:drawableTop="@drawable/tvselector"
                        />
                    <TextView
                        android:id="@+id/passLimit"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/backBatteryPosStr"
                        android:gravity="center"
                        android:drawableTop="@drawable/tvselector"
                        />
                    <TextView
                        android:id="@+id/dispensingWorkPiece"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/SpinBatteryPosStr"
                        android:gravity="center"
                        android:drawableTop="@drawable/tvselector"
                        />

                </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="460dp"
                    android:orientation="vertical"
                    android:background="@drawable/side">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:text="@string/loadBatteryStr"
                        android:textSize="20sp"
                        android:textStyle="bold"/>
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        >
                        <TextView
                            android:id="@+id/loadBatterySS"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/workPiece"
                            android:gravity="center"
                            android:drawableTop="@drawable/tvselector"
                            />
                        <TextView
                            android:id="@+id/batterySS"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/batteryStr"
                            android:gravity="center"
                            android:drawableTop="@drawable/tvselector"
                            />
                        <TextView
                            android:id="@+id/lackBatterySS"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/lackBatteryStr"
                            android:gravity="center"
                            android:drawableTop="@drawable/tvselector"
                            />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        >
                        <TextView
                            android:id="@+id/pushBatterySS"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/pushBatteryPosStr"
                            android:gravity="center"
                            android:drawableTop="@drawable/tvselector"
                            />
                        <TextView
                            android:id="@+id/backBatteryPosSS"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/backBatteryPosStr"
                            android:gravity="center"
                            android:drawableTop="@drawable/tvselector"
                            />
                        <TextView
                            android:id="@+id/SpinBatteryPosSS"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/SpinBatteryPosStr"
                            android:gravity="center"
                            android:drawableTop="@drawable/tvselector"
                            />
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="460dp"
                    android:orientation="vertical"
                    android:background="@drawable/side">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:text="@string/waterAndDispensing"
                        android:textSize="20sp"
                        android:textStyle="bold"/>
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        >
                        <TextView
                            android:id="@+id/dispensingPosSS"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/dispensing"
                            android:gravity="center"
                            android:drawableTop="@drawable/tvselector"
                            />
                        <TextView
                            android:id="@+id/grabWaterPosSS"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/grabMaterialsPosStr"
                            android:gravity="center"
                            android:drawableTop="@drawable/tvselector"
                            />
                        <TextView
                            android:id="@+id/testWaterPosSS"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/testPosStr"
                            android:gravity="center"
                            android:drawableTop="@drawable/tvselector"
                            />
                        <TextView
                            android:id="@+id/testWaterPosUpLimtSS"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/upLimtStr"
                            android:gravity="center"
                            android:drawableTop="@drawable/tvselector"
                            />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        >
                        <TextView
                            android:id="@+id/waterPieceSS"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/workPiece"
                            android:gravity="center"
                            android:drawableTop="@drawable/tvselector"
                            />
                        <TextView
                            android:id="@+id/OKSS"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/okStr"
                            android:gravity="center"
                            android:drawableTop="@drawable/tvselector"
                            />
                        <TextView
                            android:id="@+id/NGSS"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/ngStr"
                            android:gravity="center"
                            android:drawableTop="@drawable/tvselector"
                            />
                        <TextView
                            android:id="@+id/testWaterPosDownLimtSS"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/downLimtStr"
                            android:gravity="center"
                            android:drawableTop="@drawable/tvselector"
                            />
                    </LinearLayout>
                </LinearLayout>

            </TableRow>
            </TableLayout>

</LinearLayout>